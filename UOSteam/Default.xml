 <macros>
  <macro loop="True" name="zzTMP" interrupt="True">usetype 0x14ed;pause 2500;</macro>
  <macro loop="False" name="castParalizeField" interrupt="True">headmsg "Target Paralyze Field" 0xff1;cast "Paralyze Field";waitingfortarget 500;</macro>
  <macro loop="False" name="skillMeditation" interrupt="False">headmsg "Meditation" 0xff1;warmode 'off';useskill 'Meditation';</macro>
  <macro loop="False" name="skillHidingStealth" interrupt="True">warmode 'off';useskill 'Hiding';clearjournal;@clearlist 'counter';@createlist 'counter';while not @injournal 'You have hidden';pushlist 'counter' 'abc';if list 'counter' == 1;headmsg "Hiding alert 1" 0xff1;endif;if list 'counter' == 2;headmsg "Hiding alert  2" 0xff1;endif;if list 'counter' == 3;headmsg "Hiding NOW" 0xff1;endif;pause 1000;endwhile;@clearlist 'counter';clearjournal;while not @injournal 'You have been revealed';clearjournal;useskill 'Stealth';waitforjournal 'You begin to move' 4000;pushlist 'counter' 'abc';if list 'counter' == 1;headmsg "Stealth active" 0xff1;endif;if @injournal 'You must use the hiding';break;endif;endwhile;</macro>
  <macro loop="False" name="equipBow" interrupt="True">headmsg "Equip Bow + Bandage" 0xff1;bandageself;pause 600;warmode 'on';if not listexists 'WepList';createlist 'WepList';endif;@clearlist 'WepList';@pushlist 'WepList' 0x13b1 //bow;//;for 0 to 'WepList';if @findtype 'WepList[]' 'any' 'backpack';@unsetalias 'WEP';@setalias 'WEP' 'found';if not @findlayer 'self' 2;equipitem 'WEP' 2;endif;endif;endfor;attack 'last';</macro>
  <macro loop="False" name="equipWeaponShield" interrupt="True">headmsg "Equip Weap-Shil + Band" 0xff1;bandageself;pause 600;warmode 'on';if not listexists 'WepList';createlist 'WepList';endif;@clearlist 'WepList';@pushlist 'WepList' 0x1b7b //metal shield;@pushlist 'WepList' 0x1400 //kiss;@pushlist 'WepList' 0x1b76 //heatet shield;@pushlist 'WepList' 0x13fe //katana;@pushlist 'WepList' 0x143f //Helbard;@pushlist 'WepList' 0x13b9 //vivking;@pushlist 'WepList' 0xf4e //bardiche;@pushlist 'WepList' 0xf4e //bardiche;//;for 0 to 'WepList';if @findtype 'WepList[]' 'any' 'backpack';@unsetalias 'WEP';@setalias 'WEP' 'found';if not @findlayer 'self' 2;equipitem 'WEP' 2;endif;endif;endfor;attack 'last';//@Getenemy 'murderer' 'Enemy' 'criminal' 'gray' 'closest';//if @findobject 'enemy';//  autotargetobject! 'enemy';//  attack! 'enemy';//endif;//@clearusequeue;//@cancelautotarget;</macro>
  <macro loop="False" name="pvpEscapeParalize" interrupt="False">headmsg "Escape Paralize" 0xff1;if @findtype 0xf02 'any' 'self';cast 'Magic Arrow';waitfortarget 2000;target 'found';else;if mana &lt;= 90;usetype 0xf0e 1152 //potion;endif;cast 'Dispel';waitfortarget 8000;target 'self';endif;//equipitem 'found' 2;//findtype 0x13b1 'any' 'self';</macro>
  <macro loop="True" name="zztmp2" interrupt="True">useskill 'Peacemaking';pause 250;</macro>
  <macro loop="True" name="zztmp3" interrupt="True">@usetype '0x1ebc';@automenu! 'Tinkering' 'Parts' '' 'clock parts';clearjournal;if counter 'cparts' > 150;organizer 'cparts';pause 1000;endif;if counter 'iron' &lt; 30;organizer 'iron';pause 1000;endif;if skill 'Tinkering' >= 70;stop;endif;waitforjournal 'in your pack' 4000;</macro>
  <macro loop="False" name="attackClosestHostile" interrupt="True">headmsg "Attack closest hostile" 0xff1;getenemy 'murderer' 'enemy' 'criminal' 'gray' 'closest';if @findobject 'enemy';autotargetobject! 'enemy';attack! 'enemy';endif;@clearusequeue;@cancelautotarget;</macro>
  <macro loop="False" name="attackNextHostile" interrupt="True">headmsg "Attack next hostile" 0xff1;getenemy 'murderer' 'enemy' 'criminal' 'gray' 'next';if @findobject 'enemy';autotargetobject! 'enemy';attack! 'enemy';endif;@clearusequeue;@cancelautotarget;</macro>
  <macro loop="False" name="pvpTargetLast" interrupt="True">target! 'last';</macro>
  <macro loop="True" name="skillAnimalTaming" interrupt="True">if not @findobject 'animal';headmsg "Skill Animal Taming" 0xff1;warmode 'off';promptalias 'animal';endif;autotargetobject 'animal';usetype! 0xeb2 389;clearjournal;for 120;if @injournal 'falhou' or @injournal 'too far';break;endif;if @injournal 'linha de visao';break;endif;if  @injournal 'mestre' or @injournal 'belong';stop;endif;pause 100;endfor;</macro>
  <macro loop="False" name="pvpFuga" interrupt="True">headmsg "FUGA" 0xff1;usetype 0xf0e 1;msg '.recall 1 1';pause 500;bandageself;</macro>
  <macro loop="False" name="zzGoHome" interrupt="True">headmsg "Go Home" 0xff1;msg '.recall 1 1';pause 500;bandageself;</macro>
  <macro loop="False" name="skillTracking" interrupt="True">headmsg "Skill Traking" 0xff1;useskill 'Tracking';</macro>
